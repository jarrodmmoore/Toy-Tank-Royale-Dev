#red
tag @s[tag=hasSlot3] remove hasSlot3
execute if score #red_team_available value matches 1.. if entity @s[scores={shootCooldown=..0}] if items entity @s hotbar.3 *[custom_data~{slot3:1b,real:1b}] run tag @s add hasSlot3
execute if score #red_team_available value matches 1.. if entity @s[scores={shootCooldown=1..}] if items entity @s hotbar.3 *[custom_data~{slot3:1b,ghost:1b}] run tag @s add hasSlot3
execute if score #red_team_available value matches ..0 if items entity @s hotbar.3 *[custom_data~{slot3:1b,deny:1b}] run tag @s add hasSlot3
clear @s[gamemode=!creative,tag=!hasSlot3] yellow_dye[custom_data~{slot3:1b}]
clear @s[gamemode=!creative,tag=!hasSlot3] yellow_dye[custom_data~{slot4:1b}]
execute unless score #force_turtles value matches 1 if score #red_team_available value matches 1.. if entity @s[tag=!hasSlot3,tag=!noInventory,scores={shootCooldown=..0}] run item replace entity @s hotbar.3 with minecraft:yellow_dye[item_model="ttr/tanks/full_red",custom_name='["",{"translate":"ttr.hotbar.press_rmb_to_enter_game_red","color":"red","bold":true,"italic":false,"with":[{"keybind":"key.use","color":"yellow","bold":false,"italic":false}]}]',custom_data={slot3:1b,real:1b}] 1
